{
  "name": "clean-node-api",
  "version": "2.5.4",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand --detectOpenHandles --no-cache",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:staged": "jest --force-exit --passWithNoTests -c jest-staged-config.js",
    "test:ci": "npm test -- --force-exit --coverage",
    "dev": "ts-node-dev -r tsconfig-paths/register ./src/main/server.ts",
    "start": "node ./dist/main/server.js",
    "debug": "ts-node-dev  --inspect=0.0.0.0:9222 --exit-child --respawn -r tsconfig-paths/register  ./src/main/server.ts",
    "build": "rimraf ./dist && tsc -p tsconfig-build.json",
    "postbuild": "copyfiles -u 1 public/**/* dist/static",
    "test:coveralls": "npm run test:ci && coveralls < coverage/lcov.info"
  },
  "engines": {
    "node": "14.x"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "copyfiles": "^2.4.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongo-round": "^1.0.0",
    "mongodb": "^4.1.4",
    "swagger-ui-express": "^4.3.0",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^2.1.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^16.11.11",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/validator": "^13.6.6",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "coveralls": "^3.1.1",
    "eslint": "^7.12.1",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.0.0",
    "git-commit-msg-linter": "^3.2.8",
    "husky": "^4.3.8",
    "jest": "^27.3.1",
    "lint-staged": "^11.2.6",
    "mockdate": "^3.0.5",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.4.4"
  },
  "_moduleAliases": {
    "@root": "dist"
  }
}
